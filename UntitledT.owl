###  http://www.semanticweb.org/alber/ontologies/2021/11/untitled-ontology-4#AFFILIATED_PEOPLE
:AFFILIATED_PEOPLE rdf:type owl:Class ;
                   rdfs:comment "it represent the register of all the people having a role in the company"^^xsd:string ;
                   rdfs:label "AFFILIATED PEOPLE"^^xsd:string .


###  http://www.semanticweb.org/alber/ontologies/2021/11/untitled-ontology-4#COMPANY
:COMPANY rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :HAS_MANAGER ;
                           owl:someValuesFrom :MANAGER
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :HAS_SITE ;
                           owl:someValuesFrom :SITE
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :HAS__PARTECIPANT ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :PARTICIPANT_1
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :HAS__PARTECIPANT ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :PARTICIPANT_2
                         ] .


###  http://www.semanticweb.org/alber/ontologies/2021/11/untitled-ontology-4#CONTACT_PERSON
:CONTACT_PERSON rdf:type owl:Class ;
                rdfs:subClassOf :AFFILIATED_PEOPLE ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :REPRESENT ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :COMPANY
                                ] .


###  http://www.semanticweb.org/alber/ontologies/2021/11/untitled-ontology-4#COUNTRY
:COUNTRY rdf:type owl:Class ;
         rdfs:subClassOf :SITE .


###  http://www.semanticweb.org/alber/ontologies/2021/11/untitled-ontology-4#MANAGER
:MANAGER rdf:type owl:Class ;
         rdfs:subClassOf :AFFILIATED_PEOPLE ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :MANAGE ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :COMPANY
                         ] .


###  http://www.semanticweb.org/alber/ontologies/2021/11/untitled-ontology-4#PARTICIPANT_1
:PARTICIPANT_1 rdf:type owl:Class ;
               rdfs:subClassOf :AFFILIATED_PEOPLE ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :PARTECIPATE ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :COMPANY
                               ] .


###  http://www.semanticweb.org/alber/ontologies/2021/11/untitled-ontology-4#PARTICIPANT_2
:PARTICIPANT_2 rdf:type owl:Class ;
               rdfs:subClassOf :AFFILIATED_PEOPLE ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :PARTECIPATE ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :COMPANY
                               ] .


###  http://www.semanticweb.org/alber/ontologies/2021/11/untitled-ontology-4#REGION
:REGION rdf:type owl:Class ;
        rdfs:subClassOf :SITE .


###  http://www.semanticweb.org/alber/ontologies/2021/11/untitled-ontology-4#SITE
:SITE rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :LOCATION_OF ;
                        owl:someValuesFrom :COMPANY
                      ] .


